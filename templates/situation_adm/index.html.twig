{% extends 'base.html.twig' %}

{% block title %}SituationAdm index{% endblock %}

{% block body %}
    <h1>SituationAdm index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>LibelleSituation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for situation_adm in situation_adms %}
            <tr>
                <td>{{ situation_adm.id }}</td>
                <td>{{ situation_adm.libelleSituation }}</td>
                <td>
                    <a href="{{ path('app_situation_adm_show', {'id': situation_adm.id}) }}">show</a>
                    <a href="{{ path('app_situation_adm_edit', {'id': situation_adm.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_situation_adm_new') }}">Create new</a>
{% endblock %}
